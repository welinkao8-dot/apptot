{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend-monorepo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend-monorepo/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend-monorepo",
  "projects": {
    "admin-api": {
      "type": "application",
      "root": "apps/admin-api",
      "entryFile": "main",
      "sourceRoot": "apps/admin-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-api/tsconfig.app.json"
      }
    },
    "backend-monorepo": {
      "type": "application",
      "root": "apps/backend-monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/backend-monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend-monorepo/tsconfig.app.json"
      }
    },
    "client-api": {
      "type": "application",
      "root": "apps/client-api",
      "entryFile": "main",
      "sourceRoot": "apps/client-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-api/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "driver-api": {
      "type": "application",
      "root": "apps/driver-api",
      "entryFile": "main",
      "sourceRoot": "apps/driver-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/driver-api/tsconfig.app.json"
      }
    }
  }
}